{
  "name": "moviedb",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Quasar Movie Database",
  "author": "evannicholas <evannicholasabednego@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:pwa": "quasar dev -m pwa",
    "dev:android": "quasar dev -m capacitor -T android",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "dev:elc": "quasar dev -m electron",
    "build:elecns": "quasar build -m electron -- --no-sandbox --disable-setuid-sandbox",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "build:android": "quasar build -m capacitor -T android",
    "build:ios": "quasar build -m capacitor -T ios",
    "build:elec": "quasar build -m electron"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "quasar": "^2.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@quasar/app": "^3.0.0",
    "electron": "^13.1.6"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
